{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "GET /shop/list Shop.list output data",
  "properties": {
    "total": {
      "type": "integer",
      "description": "搜尋結果的總數"
    },
    "data": {
      "type": "array",
      "description": "商店資料列表",
      "items": {
        "type": "object",
        "description": "商店資料",
        "properties": {
          "public_id": {
            "type": "string",
            "description": "商店公開編號"
          },
          "name": {
            "type": "string",
            "description": "商店名稱"
          },
          "web": {
            "type": "string",
            "description": "商家網址",
            "nullable": true
          },
          "logo": {
            "type": "string",
            "description": "商家logo",
            "nullable": true
          },
          "longitude": {
            "type": "number",
            "description": "商家經度"
          },
          "latitude": {
            "type": "number",
            "description": "商家緯度"
            },
          "tel": {
            "type": "string",
            "description": "店家聯絡電話",
            "nullable": true
          },
          "address": {
            "type": "string",
            "description": "店家地址",
            "nullable": true
          },
          "category": {
            "type": "integer",
            "description": "店家類型"
          },
          "description": {
            "type": "string",
            "description": "敘述",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}