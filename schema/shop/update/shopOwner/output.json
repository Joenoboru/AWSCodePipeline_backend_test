{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "PUT /shop/:public_id Shop.update output data",
  "properties": {
    "public_id": {
      "type": "string",
      "description": "店家公開編號"
    },
    "name": {
      "type": "string",
      "description": "店家名稱"
    },
    "tel": {
      "type": "string",
      "description": "店家聯絡電話",
      "nullable": true
    },
    "address": {
      "type": "string",
      "description": "店家地址",
      "nullable": true
    },
    "longitude": {
      "type": "number",
      "description": "店家經度"
    },
    "latitude": {
      "type": "number",
      "description": "店家緯度"
    },
    "email": {
      "type": "string",
      "description": "店家email",
      "nullable": true
    },
    "category": {
      "type": "integer",
      "description": "店家類型"
    },
    "description": {
      "type": "string",
      "description": "店家敘述",
      "nullable": true
    },
    "web": {
      "type": "string",
      "description": "店家網站",
      "nullable": true
    },
    "logo": {
      "type": "string",
      "description": "店家LOGO",
      "nullable": true
    },
    "contact": {
      "type": "string",
      "description": "聯絡人",
      "nullable": true
    },
    "contact_phone": {
      "type": "string",
      "description": "聯絡人電話",
      "nullable": true
    },
    "status": {
      "type": "integer",
      "description": "店家狀態（0:未啟用, 1:啟用）"
    },
    "created_time": {
      "type": "object",
      "description": "資料新增時間"
    },
    "updated_time": {
      "type": "object",
      "description": "資料更新時間"
    }
  },
  "additionalProperties": false
} 
