{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "PUT /User/:publicId User.update input data",
  "properties": {
    "name": {
      "type": "string",
      "description": "會員姓名",
      "minLength": 1
    },
    "email": {
      "type": "string",
      "description": "會員電子信箱",
      "minLength": 1
    },
    "phone": {
      "type": "string",
      "description": "會員手機號碼"
    },
    "birth_year": {
      "type": "integer",
      "description": "會員出生年"
    },
    "birth_month": {
      "type": "integer",
      "description": "會員出生月"
    },
    "birth_day": {
      "type": "integer",
      "description": "會員出生日"
    },
    "gender": {
      "type": "integer",
      "description": "會員性別（1: 男, 2: 女）"
    },
    "city": {
      "type": "string",
      "description": "會員居住縣市"
    },
    "picture": {
      "type": "string",
      "description": "會員照片"
    },
    "passport_name": {
      "type": "string",
      "description": "護照姓名"
    },
    "emergency_contact": {
      "type": "string",
      "description": "緊急聯絡人姓名"
    },
    "emergency_number": {
      "type": "string",
      "description": "緊急聯絡人電話"
    },
    "custom_id": {
      "type": "string",
      "description": "客戶編號"
    },
    "notes": {
      "type": "string",
      "description": "額外資訊"
    },
    "address": {
      "type": "string",
      "description": "會員地址"
    }
  },
  "additionalProperties": false,
  "required": [
  ]
}
