    "line_id": {
      "type": "string",
      "description": "會員 line 編號",
      "nullable": true
    },
    "name": {
      "type": "string",
      "description": "會員姓名",
      "nullable": true
    },
    "email": {
      "type": "string",
      "description": "會員電子信箱",
      "nullable": true
