    "birth_month": {
      "type": "integer",
      "description": "會員出生月",
      "nullable": true
    },
    "birth_day": {
      "type": "integer",
      "description": "會員出生日",
      "nullable": true
    },
    "gender": {
      "type": "integer",
      "description": "會員性別（1: 男, 2: 女）"
