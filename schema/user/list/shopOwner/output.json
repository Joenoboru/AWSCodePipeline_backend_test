{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "GET /user/list User.list output data",
  "properties": {
    "total": {
      "type": "integer",
      "description": "搜尋結果的總數"
    },
    "data": {
      "type": "array",
      "description": "多個會員資料",
      "items": {
        "type": "object",
        "description": "會員資料",
        "properties": {
          "public_id": {
            "type": "string",
            "description": "會員公開編號"
          },
          "name": {
            "type": "string",
            "description": "會員名稱",
            "nullable": true
          },
          "email": {
            "type": "string",
            "description": "會員Email",
            "nullable": true
          },
          "picture": {
            "type": "string",
            "description": "會員照片",
            "nullable": true
          },
          "gender": {
            "type": "number",
            "description": "會員性別 (0: 未選擇, 1: 男, 2:女 )"
          },
          "birth_day": {
            "type": "string",
            "description": "會員生日"
          },
          "city": {
            "type": "string",
            "description": "居住城市",
            "nullable": true
          },
          "address": {
            "type": "string",
            "description": "居住地址",
            "nullable": true
          },
          "phone": {
            "type": "string",
            "description": "會員手機號碼",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}