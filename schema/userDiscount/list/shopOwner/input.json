{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "GET /userDiscount/list UserDiscount.list input data",
  "properties": {
    "limit": {
      "type": "integer",
      "description": "指定此次搜尋結果的回傳資料筆數"
    },
    "offset": {
      "type": "integer",
      "description": "指定要從第幾筆資料回傳"
    },
    "search": {
      "type": "string",
      "description": "找尋discount_name、shop_name中含有相關文字的資料"
    },
    "sort": {
      "type": "string",
      "description": "指定資料排序方式"
    },
    "class": {
      "type": "integer",
      "description": "活動類型分類(0: 活動 1: 商品劵)"
    }
  },
  "additionalProperties": true
}