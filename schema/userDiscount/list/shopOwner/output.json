{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "GET /userDiscount/list UserDiscount.list output data",
  "properties": {
    "total": {
      "type": "integer",
      "description": "搜尋結果的總數"
    },
    "data": {
      "type": "array",
      "description": "商家的折扣與商品卷 發出/核銷折扣列表",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "會員擁有折扣的編號"
          },
          "activity_class": {
            "type": "integer",
            "description": "活動類型分類"
          },
          "user_name": {
            "type": "string",
            "description": "會員姓名"
          },         
          "user_email": {
            "type": "string",
            "description": "會員電子信箱"
          },
          "discount_name": {
            "type": "string",
            "description": "折扣名稱"
          },         
          "created_time": {
            "type": "object",
            "description": "資料新增時間"
          },
          "use_time": {
            "type": "object",
            "description": "實際使用時間",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "description": "狀態(0: 未使用 1: 已使用 2: 作廢)"
          },
          "code": {
            "type": "string",
            "description": "折扣條碼"
          },
          "serial_num": {
            "type": "string",
            "description": "流水號",
            "nullable": true
          }
        },
        "additionalProperties": false
      }    
   	}
  },
  "additionalProperties": false
}