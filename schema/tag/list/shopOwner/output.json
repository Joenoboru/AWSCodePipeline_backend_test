{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "GET /tag/list tag.list output data",
  "properties": {
    "total": {
      "type": "integer",
      "description": "搜尋結果的總數"
    },
    "data": {
      "type": "array",
      "description": "使用者折扣列表",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "標籤的編號"
          },
          "name": {
            "type": "string",
            "description": "標籤名稱"
          },
          "type": {
            "type": "integer",
            "description": "標籤類型(0: 活動標籤 1: 會員標籤)"
          }
        },
        "additionalProperties": false
      }    
    }
  },
  "additionalProperties": false
}