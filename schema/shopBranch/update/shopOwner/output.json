{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "PUT /shopBranch/:public_id shopBranch.update output data",
  "properties": {
    "public_id": {
      "type": "string",
      "description": "分店資訊公開編號"
    },
    "name": {
      "type": "string",
      "description": "分店名稱"
    },
    "status": {
      "type": "integer",
      "description": "分店狀態（0:未啟用, 1:啟用）"
    },
    "created_time": {
      "type": "object",
      "description": "資料新增時間"
    },
    "updated_time": {
      "type": "object",
      "description": "資料更新時間"
    }
  },
  "additionalProperties": false
} 
