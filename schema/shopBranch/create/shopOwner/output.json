{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "POST /shopBranch shopBranch.create output data",
  "properties": {
    "public_id": {
      "type": "string",
      "description": "分店資訊公開編號"
    },
    "shop_id": {
      "type": "integer",
      "description": "店家編號"
    },
    "name": {
      "type": "string",
      "description": "分店名稱"
    },
    "status": {
      "type": "integer",
      "description": "分店狀態(0: 關閉 1: 開啟）"
    },
    "created_time": {
      "type": "object",
      "description": "資料新增時間"
    },
    "updated_time": {
      "type": "object",
      "description": "資料更新時間"
    }
  },
  "additionalProperties": false
} 
